{
  "name": "volt",
  "version": "0.0.1",
  "author": "Andrew Kidoo",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pack": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "react-app-rewired test",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build && xcopy /Y .\\public\\electron.js .\\build\\"
  },
  "build": {
    "appId": "com.kustomzone.volt",
    "productName": "Volt Coding Agent",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "icon": "assets/icon.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@codesandbox/sandpack-client": "2.19.8",
    "@codesandbox/sandpack-react": "^2.19.10",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@codesandbox/sdk": "^0.0.6",
    "@conform-to/zod": "^1.1.5",
    "@electron/remote": "^2.1.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@langchain/anthropic": "^0.3.11",
    "@langchain/core": "^0.3.29",
    "@langchain/google-genai": "^0.1.6",
    "@langchain/groq": "^0.1.3",
    "@langchain/ollama": "^0.1.4",
    "@langchain/openai": "^0.3.17",
    "@langchain/xai": "^0.0.1",
    "@prisma/client": "^6.2.0",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@vercel/og": "^0.6.2",
    "ai": "^4.0.22",
    "concurrently": "^9.1.2",
    "dedent": "^1.5.3",
    "electron-is-dev": "^2.0.0",
    "eventsource-parser": "^1.1.2",
    "framer-motion": "^11.3.19",
    "gpt-tokenizer": "^2.8.1",
    "langchain": "^0.3.11",
    "lucide-react": "^0.469.0",
    "next": "^15.1.4",
    "ollama-ai-provider": "^1.1.0",
    "openai": "^4.77.3",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "shadcn-ui": "^0.9.4",
    "sonner": "^1.5.0",
    "tailwindcss-animate": "^1.0.7",
    "wait-on": "^8.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^6.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2"
  }
}
